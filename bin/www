#!/usr/bin/env node
var debug = require('debug')('app2');
var app = require('../app');
//var app1 = require('../app1');

app.set('port', process.env.PORT || 80);

var server = app.listen(app.get('port'), function() {
  debug('RESTful server listening on port ' + server.address().port);
});

